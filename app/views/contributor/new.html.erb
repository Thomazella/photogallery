<% if admin_logged_in? %>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_for(@contributor, url: new_contributor_path) do |f| %>

        <p> This will create a new default contributor with: </p>
        <br>
          <p> login = contributor[hash] </p>
          <p> password = password[hash] </p>
          <p> name = contributor[hash] </p>
          <p> email = contributor[hash]@example.com </p>
        <br>
        <%= f.submit "Add Contributor", class: "btn btn-primary" %>
      <% end %>

    </div>
  </div>
<% else %>
  This is an admin area.
<% end %>
